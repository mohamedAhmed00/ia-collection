{
    "name": "mohamedahmed00/ia-collection",
    "type": "library",
    "description": "Laravel Illuminate Agnostic Collection",
    "keywords": [
        "mohamedahmed00",
        "collection",
        "helpers",
        "laravel"
    ],
    "homepage": "https://github.com/antonioribeiro/ia-collection",
    "license": "MIT",
    "authors": [
        {
            "name": "mohamed ahmed",
            "email": "engmohamedahmed00@gmail.com",
            "homepage": "https://antoniocarlosribeiro.com",
            "role": "Creator"
        }
    ],
    "require": {
        "php": "^8.2",
        "nesbot/carbon": "^3.8",
        "ramsey/uuid": "^4.7",
        "symfony/var-dumper": "^7.1",
        "voku/portable-ascii": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "squizlabs/php_codesniffer": "^3.10",
        "mockery/mockery": "^1.6",
        "moontoast/math": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "IlluminateAgnostic\\Collection\\": "src/",
            "IlluminateAgnostic\\Collection\\Tests\\": "tests/"
        },
        "files": [
            "src/Support/helpers.php",
            "src/Support/alias.php"
        ]
    },
    "scripts": {
        "test": [
            "@composer install",
            "vendor/bin/phpunit"
        ],
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
